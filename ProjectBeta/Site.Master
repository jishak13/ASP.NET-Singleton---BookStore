                        <div class="search-full-view">
                            <div class="input-group">
                                <asp:Panel ID="panSearch" runat="server" DefaultButton="searchButton">                                  
                                    <asp:TextBox ID="searchTextBox" runat="server" CssClass="form-control" ClientIDMode="Inherit" BorderStyle="Solid" BorderColor="White" BorderWidth="1px"></asp:TextBox>
                                    <asp:LinkButton ID="searchButton" runat="server" CssClass="input-group-addon" OnClick="searchButton_Click" BorderStyle="None" BorderColor="Black">
                                        <i class="fa fa-search" aria-hidden="true"></i>
                                    </asp:LinkButton>
                                    <br />
                                    <br />
                                    <br />
                                    <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ErrorMessage="Please enter search criteria!" ControlToValidate="searchTextBox" Visible="True" Font-Size="X-Large" ForeColor="#B78C33"></asp:RequiredFieldValidator>
                                </asp:Panel>   
                            
                                <asp:Label ID="previousSearchLabel" runat="server" Visible="False" ForeColor="#B78C33" Font-Size="Large"></asp:Label>
                                <%--<asp:Label ID="previousSearchLabel" runat="server" Visible="False" Font-Size="Medium"></asp:Label>--%>
                                <%--<img src="data:image/png;base64,">--%>                              <%--</div>--%>
